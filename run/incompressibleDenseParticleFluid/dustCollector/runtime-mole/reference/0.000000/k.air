FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.000000";
    object      k.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 1;
    }
    "wing-1|wing-2|wing-3"
    {
        type            kqRWallFunction;
        value           uniform 1;
    }
    outlet-screw-1
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 1;
        value           uniform 1;
    }
    outlet-screw-2
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 1;
        value           uniform 1;
    }
    outlet-cyclone
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 1;
        value           uniform 1;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
}


