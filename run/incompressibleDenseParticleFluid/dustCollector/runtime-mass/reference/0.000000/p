FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.000000";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    "wing-1|wing-2|wing-3"
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    outlet-screw-1
    {
        type            fixedValue;
        value           uniform -389.63;
    }
    outlet-screw-2
    {
        type            fixedValue;
        value           uniform -430.64;
    }
    outlet-cyclone
    {
        type            fixedValue;
        value           uniform -816.16;
    }
    inlet
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
}


