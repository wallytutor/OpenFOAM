FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.000000";
    object      omega.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 100;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            omegaWallFunction;
        value           uniform 100;
    }
    "wing-1|wing-2|wing-3"
    {
        type            omegaWallFunction;
        value           uniform 100;
    }
    outlet-screw-1
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 100;
        value           uniform 100;
    }
    outlet-screw-2
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 100;
        value           uniform 100;
    }
    outlet-cyclone
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 100;
        value           uniform 100;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 100;
    }
}


