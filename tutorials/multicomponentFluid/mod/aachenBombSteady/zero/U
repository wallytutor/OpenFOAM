FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// ************************************************************************* //

dimensions          [0 1 -1 0 0 0 0];

internalField       uniform (0 0 0);

boundaryField
{
    inlet
    {
        type                flowRateInletVelocity;
        massFlowRate        constant 0.030;
        profile             turbulentBL;
    }
    outlet
    {
        type                inletOutlet;
        inletValue          uniform (0 0 0);
    }
    walls
    {
        type                fixedValue;
        value               uniform (0 0 0);
    }
}

// ************************************************************************* //
