FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       foamRun;

solver            multicomponentFluid;

startFrom         latestTime;

startTime         0;

stopAt            endTime;

endTime           200;

deltaT            1;

writeControl      adjustableRunTime;

writeInterval     20;

purgeWrite        10;

writeFormat       binary;

writePrecision    6;

writeCompression  off;

timeFormat        fixed;

timePrecision     0;

adjustTimeStep    yes;

maxCo             0.1;

runTimeModifiable yes;

functions
{
    #includeFunc Qdot
}

OptimisationSwitches
{
    fileHandler collated;
}

